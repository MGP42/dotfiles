set background=dark
:colorscheme dki
:syntax on
:filetype indent on
set mouse=a
set viminfo=""
set number
set linebreak
set visualbell
set autoindent
set softtabstop=4
set tabstop=4
set shiftwidth=4
set ruler

map <C-b> :NERDTreeToggle<CR>
set laststatus=2
set statusline=%<%f\ %h%m%r%=%-14.(%l,%c%V%)
