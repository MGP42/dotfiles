			"\\usepackage{amsmath}",
			"\\usepackage{amssymb}",
			"\\usepackage{hyperref}",
			"",
			"\\newcommand{\\script}[1]{\\includegraphics[page=#1, width=15cm]{../Script/Script.pdf}}",
			"",
			"\\begin{document}",
			"",
			"\\end{document}"
		],
		}
